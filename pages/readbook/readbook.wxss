.content image{
  height: 300rpx;
}
.wraper{
margin-top: 10rpx;
padding:0  3%
}
.title{
  text-align: center  
}
.textarea{
  font-size: 22rps;
  line-height: 1.5;
  text-indent: 2;
}
.text{
  font-size: 30rpx;
  color: #333;
  padding: 2rpx;
  padding-bottom: 100rpx;
}
.bottomnav{
  display: flex;
  justify-content: space-around;
  height: 100rpx;
  line-height: 100rpx;
  z-index: 100;
  width: 100%;
  border-top: 1px solid #f2f2f2;
  background-color: #fff;
  position: fixed;
  left: 0;
  bottom: 0;

}
.bottomnav icon{
  font-size: 50rpx;
  color: #333;
  width: 50rpx;

}

.bottomnav view{
  width: 50rpx;
}

.bottomnav view:active{
  background-color: #f2f2f2;


}
.nav-daohang{
  width: 70%;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  z-index: 999;
  background-color: #fff;
 transform: translate3d(-100%,0,0);
  transition: transform 0.3s linear;
}
.doing{
transform:translate3d(0,0,0);

}
.nav-daohang view{
  height: 80rpx;
  z-index: 999;
  line-height: 80rpx;
  font-size: 27rpx;
  color: #666;
  padding: 0 10rpx;
}

.mask{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 998;
  background-color: rgba(0, 0, 0, .3)
}